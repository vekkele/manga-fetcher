    "@tauri-apps/cli": "^1.0.0-rc.9",
    "@tsconfig/svelte": "^2.0.1",
    "smui-theme": "^6.0.0-beta.16",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.7",
    "svelte-material-ui": "^6.0.0-beta.16",
    "svelte-preprocess": "^4.9.8",
